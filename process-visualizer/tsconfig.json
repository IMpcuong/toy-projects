{
  "extends": "@tsconfig/deno/tsconfig.json",
  "compilerOptions": {
    "module": "System",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true
  },
  "include": [
    "src/fe/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}