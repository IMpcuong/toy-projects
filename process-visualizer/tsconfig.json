{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "lib": [
      "DOM",
      "ES2017",
      "ESNext",
    ],
    "jsx": "react",
    "module": "ESNext",
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "src/fe/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}